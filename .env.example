# Example environment variables for nuxt4-boilerplate-nuxt-ui
# Copy this file to .env and fill in your own values

# Authentication Configuration
NUXT_AUTH_SECRET=your-super-secret-jwt-key-here
NUXT_AUTH_ADMIN_USERNAME=admin
NUXT_AUTH_ADMIN_PASSWORD=admin123

# SSH private key encryption secret (min 32 chars). Required for keypair generation.
NUXT_SSH_PRIVATE_KEY_SECRET=replace_with_random_32+_char_value
